{
    "name": "Replit Auto Forker",
    "description": "Forks replits and gets invite links automatically",
    "version": "0.0.2",
    "manifest_version": 3,
    "host_permissions": [
        "*://replit.com/"
    ],
    "content_scripts": [
        {
            "matches": ["https://replit.com/@*/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {"128": "icon.png"}
}